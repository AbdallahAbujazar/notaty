<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Notaty - Notes</title>
  </head>
  <body>
    <h1>Notaty - Notes</h1>
    <form onsubmit="addNote(); return false;">
        <div>
          <label for="title">Title:</label>
          <input type="text" id="title" name="title">
        </div>
        <div>
          <label for="content">Content:</label>
          <textarea id="content" name="content"></textarea>
        </div>
        <button type="submit">Add Note</button>
      </form>
      
      
    <table>
      <thead>
        <tr>
          <th>ID</th>
          <th>Title</th>
          <th>Content</th>
          <th>Created Date</th>
          <th>Updated Date</th>
        </tr>
      </thead>
      <tbody>
        <% notes.forEach(function(note) { %>
          <tr>
            <td><%= note.id %></td>
            <td><%= note.title %></td>
            <td><%= note.content %></td>
            <td><%= note.created_date %></td>
            <td><%= note.updated_date %></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>
